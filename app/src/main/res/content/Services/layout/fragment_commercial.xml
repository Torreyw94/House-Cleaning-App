<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/servicesCommercial"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Dark_Grey"
    tools:context=".MainActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingEnd="15dp"
        android:paddingStart="15dp"
        android:background="@color/Dark_Grey"
        android:theme="@style/Theme.MargaritaHouseCleaning.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_cleaning_services"
            android:layout_width="match_parent"
            app:theme="@style/ToolbarTheme"
            app:title="Services"
            app:navigationIcon="@drawable/ic_arrow_back"
            android:layout_height="?attr/actionBarSize"
            app:popupTheme="@style/Theme.MargaritaHouseCleaning.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:scrollbars="none"
        tools:layout_editor_absoluteX="2dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.03" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="393dp"
                app:layout_constraintGuide_percent="0.95" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.05" />

            <TextView
                android:id="@+id/houseCleaningTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Commercial Cleaning"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toTopOf="@+id/guideline29" />

            <TextView
                android:id="@+id/roomsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:text="All Office Rooms"
                android:textColor="#FF2929"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/imageView11" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="200dp"
                android:layout_height="54dp"
                android:layout_marginTop="32dp"
                android:text=" • Dust office furniture, lamps, woodwork, furniture, shelve, tables, and baseboards."
                android:textColor="@color/transparentWhite"

                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/roomsText" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text=" • Remove webs. "

                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <View
                android:id="@+id/divider7"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="88dp"
                android:background="@drawable/line_shape"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img7" />

            <TextView
                android:id="@+id/bathroomText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:text="Bathrooms"
                android:textColor="#FF2929"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="85dp"
                android:layout_height="2dp"
                android:layout_marginTop="4dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.003"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/bathroomText"
                app:srcCompat="@drawable/white_gray_line" />


            <TextView
                android:id="@+id/textView28"
                android:layout_width="186dp"
                android:layout_height="36dp"

                android:layout_marginTop="32dp"
                android:text=" • Clean, scrub and sanitize showers, bathtubs, and sinks. "
                android:textColor="@color/transparentWhite"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/bathroomText" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="199dp"
                android:layout_height="wrap_content"

                android:layout_marginTop="16dp"
                android:text=" • Clean and sanitize vanities, backsplashes and toilets."
                android:textColor="@color/transparentWhite"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView30" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginTop="16dp"
                android:layout_marginEnd="212dp"
                android:text=" • Clean mirrors"
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView28" />

            <ImageView
                android:id="@+id/commercial_img6"
                android:layout_width="176dp"
                android:layout_height="258dp"
                android:layout_marginTop="28dp"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img4"
                app:srcCompat="@drawable/office_hallway" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="72dp"
                android:text=" • Deodorize"
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/textView32" />

            <ImageView
                android:id="@+id/commercial_img8"
                android:layout_width="176dp"
                android:layout_height="258dp"
                android:layout_marginTop="28dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img6"
                app:srcCompat="@drawable/office_elevator1" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="128dp"
                android:layout_height="37dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="20dp"
                android:text=" • Wash floors and tile walls. "
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintStart_toStartOf="@+id/textView31"
                app:layout_constraintTop_toBottomOf="@+id/bathroomText" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginTop="16dp"
                android:layout_marginEnd="56dp"
                android:text=" • Empty Trash"
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/textView14" />

            <ImageView
                android:id="@+id/commercial_img7"
                android:layout_width="176dp"
                android:layout_height="258dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img5"
                app:srcCompat="@drawable/office_breakroom" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="134dp"
                android:layout_height="36dp"
                android:layout_marginTop="32dp"
                android:text=" • Sweep and Mop all floors "
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView14"
                app:layout_constraintTop_toBottomOf="@+id/roomsText" />

            <ImageView
                android:id="@+id/commercial_img1"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:clickable="true"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textGallery"
                app:srcCompat="@drawable/office01" />

            <ImageView
                android:id="@+id/commercial_img3"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img1"
                app:srcCompat="@drawable/office02" />

            <ImageView
                android:id="@+id/commercial_img2"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/textGallery"
                app:srcCompat="@drawable/office03" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="• Vaccum carpets."
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text=" • Vaccum furniture. "
                android:textColor="@color/transparentWhite"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView15"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="125dp"
                android:layout_height="2dp"
                android:layout_marginTop="4dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/roomsText"
                app:srcCompat="@drawable/white_gray_line" />


            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="368dp"
                android:layout_height="174dp"
                android:layout_marginTop="20dp"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/houseCleaningTitle"
                app:srcCompat="@drawable/commercial_clean_img1" />

            <TextView
                android:id="@+id/textGallery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:layout_marginEnd="16dp"
                android:text="Gallery"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/textView29" />

            <ImageView
                android:id="@+id/commercial_img4"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img2"
                app:srcCompat="@drawable/office04" />

            <ImageView
                android:id="@+id/commercial_img5"
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="@+id/guideline12"
                app:layout_constraintTop_toBottomOf="@+id/commercial_img3"
                app:srcCompat="@drawable/office05" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
